	public static void main(String[] args) {
		Scanner in = new Scanner(System.in);
		String line = "Yes";
		VowelCounter vc;
		
		while(line.equals("Yes")) {
			line = in.next();
			System.out.println("You entered " + line);
			vc = new VowelCounter(line);
			
			
		}
		

	}

}
