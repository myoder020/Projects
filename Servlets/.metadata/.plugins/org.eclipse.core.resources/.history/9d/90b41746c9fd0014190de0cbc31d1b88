package door;

public class DogDoorSimulator {
	
	public static void main(String[] args) {
		DogDoor door = new DogDoor();
		Remote remote = new Remote(door);
		System.out.println("Fido barks to go outisde...");
		remote.pressButton();
		System.out.println("\nFido barks to go outisde...");
		remote.pressButton();
		System.out.println("\nFido barks to go outisde...");
		remote.pressButton();
		System.out.println("\nFido barks to go outisde...");
		remote.pressButton();
	}
	
}
